<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/lib/bootstrap-5.0.2-dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/styles.css" />
    <link rel="stylesheet" href="~/lib/cute-alert/style.css" />
@*    <script src="https://kit.fontawesome.com/e08732690d.js" crossorigin="anonymous"></script>*@
    <script src="@Url.Content("~/lib/cute-alert/cute-alert.js")"></script>
    <script src="@Url.Content("~/lib/ckeditor4-basic/ckeditor.js")"></script>

</head>
<body>
    <div class="divwrapper">
        <!-- start divwrapper-->

        <header class="item-a">
            <nav class="navbar navbar-expand-lg navbar-dark bg-primary border-bottom" id="navmenubox">
                <a class="navbar-brand text-info" id="menulogo" asp-controller="Frontend" asp-action="Index">
                    <img alt="logo" src="@Url.Content("~/pictures/Smmslogo.png")">
                </a>
                @if (User.IsInRole("SuperAdmin") || User.IsInRole("Admin") || User.IsInRole("User"))
                {
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="collapsibleNavbar">
                    <ul class="navbar-nav">
                       
                        <li class="nav-item-admin">
                            <a class="nav-link" asp-controller="Frontend" asp-action="UserBoatData">Dina Båtar</a>
                        </li>
                        <li class="nav-item-admin">
                            <a class="nav-link" asp-controller="Frontend" asp-action="UserServiceApplications">Ansökningar Tjänster</a>
                        </li>

                    </ul>
                </div>
                }
                <div>
                    <partial name="_LoginPartial" />
                </div>

                <div id="actionMessage">@ViewBag.Mess</div>

            </nav>
        </header>

        <div class="item-e_content">
            @RenderBody()

        </div>


    </div> <!-- end divwrapper-->


    <footer class="item-h">
        <span>&#64;Eric "Zeddan" Rönnhult&nbsp;&nbsp;&nbsp;&nbsp;<a class="btn btn-primary" href="#">Back To Top</a></span>
    </footer>

    <script src="@Url.Content("~/js/jquery-3.6.0.min.js")"></script>
    <script src="@Url.Content("~/lib/bootstrap-5.0.2-dist/js/bootstrap.bundle.js")"></script>

    <script>
        function setElementInnerHtml(elemId, theHtml) {
            return document.getElementById(elemId).innerHTML = theHtml
        }
    </script>

    @if (IsSectionDefined("scripts"))
        {
        RenderSection("scripts");
        }

 

</body>
</html>