@model CreateWinterstoreSpotViewModel

<div id="adminboatslip">
    @*    <h4>Skapa ny Hamn</h4>*@



    <div>
        <form class="createeditforms" asp-action="CreateWinterstoreSpot" method="post" role="form">
            <fieldset id="fieldsetcreateharbour">
                <legend id="legendcreateharbour">Skapa ny VinterförvaringsPlats:</legend>
                @Html.AntiForgeryToken()

                <br />

                <label asp-for="Label"></label>
                <br />
                <input asp-for="Label" />
                <br />
                <span id="redtext" asp-validation-for="Label"></span>
                <br />
                <br />
                <label asp-for="Length"></label>
                <br />
                <input asp-for="Length" />
                <br />
                <span id="redtext" asp-validation-for="Length"></span>
                <br />
                <br />
                <label asp-for="Width"></label>
                <br />
                <input asp-for="Width" />
                <br />
                <span id="redtext" asp-validation-for="Width"></span>
                <br />
                <br />
                <label asp-for="Height"></label>
                <br />
                <input asp-for="Height" />
                <br />
                <span id="redtext" asp-validation-for="Height"></span>
                <br />
                <br />
                <label asp-for="ServiceTypeId"></label>
                <br />
                <select asp-for="ServiceTypeId">
                    <option value="" disabled selected>Välj FörvaringsTyp</option>
                        @{
                            if (Model.ServiceTypeListView != null)
                                {
                                    foreach (ServiceType serviceType in Model.ServiceTypeListView)
                                    {
                                        <option value="@serviceType.Id">@serviceType.Label</option>
                                    }
                                }
                        }
                </select>
@*                <div>
                    <label asp-for="StoreType"></label>
                    <br />
                    <input asp-for="StoreType" type="radio" value="Inomhus" /> <b> Inomhus</b>
                    <br />
                    <input asp-for="StoreType" type="radio" value="Utomhus" /> <b> Utomhus</b>
                    <br />
                    <span id="redtext" asp-validation-for="StoreType"></span>
                </div>*@
                <br />
                <br />
                <input type="submit" class="btn btn-primary" name="btnFilter" value="Skapa" />
            </fieldset>
        </form>

    </div>






</div>

